*{	
	margin: 0;
	padding: 0;
}
body{
	padding: 0;
	font-family: "";
}
a{
	text-decoration: none;
	/*style:none;*/	
}
i{
	style:none;
	font-style: normal;
}



/*01*/
.headph{
	position: fixed;
	top: 0;
	width: 100%;
	height: 0.88rem;
	background-color: #f8f8f8;
	line-height: 0.88rem;
    text-align: center;
    font-size: 0.4rem;
    color: #d34647;
}
.headph .heado{
	float: left;
	padding-left: 0.3rem;
}
.headph .headi{
	text-align: center;
	style:none;
	color: #000000;
}
/*02*/
.nameL{
	width: 25%;
	padding-left: 5%;
	float: left;
	height: 1.5rem;
	margin-top: 1.3rem;
	color: #b5b5b5;
	text-align: center;
	text-align: left;
	/*padding-left: 0.5rem;*/
}
.nameL i{
	display: block;
	height: 1rem;
	font-size: 0.3rem;
	line-height: 1rem;
	
}

.nameR{
	width: 70%;
	float: left;
	margin-top: 1.3rem;
	text-align: center;
}
input{
	font-size: 0.2rem;
	background-color: #f5f5f5;
	color: #999999;
	border: none;
	margin-left: 3%;
	text-indent: 0.3rem;
	outline: none;
}
.input1{
	width: 91%;
	display: block;
	height: 0.7rem;
	margin-top: 0.2rem;
}
.input2{
	width: 50%;
	display: block;
	float: left;
	height: 0.7rem;
	margin-top: 0.35rem;
}
.select{
	/*margin-left: 5%;*/
	font-size: 0.1rem;
	width: 33%;
	height: 0.7rem;
	margin-top: 0.35rem;
	outline: none;
	border: none;
	font-size: 0.24rem;
	background-color: #F5F5F5;
}
.select option{
	width: 33%;
	height: 0.4rem;
	font-size: 0.24rem;
}

.textarea{
	display: block;
	width: 80%;
	margin-left: 3%;
	margin-top: 0.4rem;
	border: none;
	outline: none;
	resize: none;
	
	background-color: #F5F5F5;
	height: 1.5rem;
	color: #999999;
	padding: 0.3rem;
}


/*03*/
.releve{
	position: fixed;
	/*bottom: 0.5rem;*/
	top: 11rem;
	height: 0.9rem;
	line-height: 0.9rem;
	text-align: center;
	border: solid #B5B5B5 0.03rem;
	width: 79%;
	margin: 0 10%;
	
}
.releve i{
	color: #000000;
	font-size: 0.33rem;
}
