body,ol,ul,h1,h2,h3,h4,h5,h6,p,th,td,dl,dd,form,fieldset,legend,input,textarea,select{
			margin:0;
			padding:0;
			
			} 
			body{font:0.16rem  "微软雅黑", HELVETICA;background:#fff; 
			-webkit-tap-highlight-color:rgba(0,0,0,0);
			-o-tap-highlight-color:rgba(0,0,0,0)
			-os-tap-highlight-color:rgba(0,0,0,0);
			tap-highlight-color:rgba(0,0,0,0);
			overflow-x:hidden ; 
			display: none;
			}
			a{text-decoration:none} 
			a:hover{text-decoration:underline} 
			em{font-style:normal}
			li{list-style:none}
			img{border:0;vertical-align:middle} 
			table{border-collapse:collapse;border-spacing:0}
			p{word-wrap:break-word}
			
			/*header{
				
				position: fixed;
				top: 0;
				z-index: 10000;
			}*/
			body {
    /**设置内嵌的元素在 3D 空间如何呈现：保留 3D*/
    -webkit-transform-style: preserve-3d;
    /**（设置进行转换的元素的背面在面对用户时是否可见：隐藏）*/
    -webkit-backface-visibility: hidden;
    
      -webkit-scrollbar {width: 0;} /* 滚动条整体部分 */
			-webkit-scrollbar-track {background-color: #fff;color: #fff;} /* 滚动条的轨道（里面装有Thumb） */
			-webkit-scrollbar-thumb {background-color: #fff;color: #fff;} /* 滑块颜色 */
			-webkit-scrollbar-button {background-color: #fff;color: #fff;} /* 滚动条的轨道的两端按钮 */
			-webkit-scrollbar-corner {background-color: #fff;color: #fff;} /* 边角，即两个滚动条的交汇处 */
			
			-o-scrollbar {width: 0;} /* 滚动条整体部分 */
			-o-scrollbar-track {background-color: #fff;color: #fff;} /* 滚动条的轨道（里面装有Thumb） */
			-o-scrollbar-thumb {background-color: #fff;color: #fff;} /* 滑块颜色 */
			-o-scrollbar-button {background-color: #fff;color: #fff;} /* 滚动条的轨道的两端按钮 */
			-o-scrollbar-corner {background-color: #fff;color: #fff;} /* 边角，即两个滚动条的交汇处 */
			
			-os-scrollbar {width: 0;} /* 滚动条整体部分 */
			-os-scrollbar-track {background-color: #fff;color: #fff;} /* 滚动条的轨道（里面装有Thumb） */
			-os-scrollbar-thumb {background-color: #fff;color: #fff;} /* 滑块颜色 */
			-os-scrollbar-button {background-color: #fff;color: #fff;} /* 滚动条的轨道的两端按钮 */
			-os-scrollbar-corner {background-color: #fff;color: #fff;} /* 边角，即两个滚动条的交汇处 */
}

a,img,button,input,textarea{-webkit-tap-highlight-color:rgba(255,255,255,0); font-size: 0.24rem;border-radius: 0;}
input,textarea{-webkit-appearance:none;}
a{
	text-decoration: none;
}
