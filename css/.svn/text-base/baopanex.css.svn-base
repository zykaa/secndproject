/*EX*/
.wodebaopanEX{
	margin-top: 0.36rem;
	width: 7.5rem;
	float: left;
	
	/*height: ;*/
}
.wodebaopanEX .wodebaopanList{
	width: 6.9rem;
	/*margin: 0 auto;*/
	margin-left: 0.3rem;
	float: left;
}
.wodebaopanEX .wodebaopanList li{
	width: 6.9rem;
	margin-top: 0.3rem;
	height: 0.65rem;
}
.wodebaopanEX .wodebaopanList li p:nth-child(1){
	width: 1.3rem;
	height: 0.65rem;
	text-align: left;
	float: left;
	line-height: 0.65rem;
	font-size: 0.28rem;
	color: #333333;
}
.wodebaopanEX .wodebaopanList li .wodebaopanbg{
	width: 5.6rem;
	height: 0.65rem;
	float: left;
	background: #E5E5E5;
}
.wodebaopanEX .wodebaopanList li .wodebaopanbg input{
	width: 4rem;
	height: 0.65rem;
	background: #E5E5E5;
	font-size: 0.24rem;
	color: #999999;
	float: left;
	text-indent: 0.3rem;
	outline: none;
	border: none;
}
.wodebaopanEX .wodebaopanList li .wodebaopanbg p{
	/*width:1.3rem ;*/
	height: 0.65rem;
	text-align: right;
	float: left;
	line-height: 0.65rem;
}
.wodebaopanEX .wodebaopanList li .wodebaopanbg p img{
	
	width:0.2rem;
	height: 0.26rem;
}
.wodebaopanEX .wodebaopanList li .wodebaopanbg p span{
	font-size: 0.24rem;
	color: #333333;
	
}