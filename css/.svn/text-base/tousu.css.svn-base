/*发布投诉*/
.tousushangpin{
	width: 7.5rem;
	float: left;
	border-bottom: 1px solid #E5E5E5;
}
.tousushangpin ul{
	width: 6.9rem;
	margin: 0 auto;
	    float: left;
    margin-left: 0.3rem;
}
.tousushangpin ul li{
	float: left;
	margin-top: 0.35rem;
	margin-bottom: 0.35rem;
}
.tousushangpin ul li:nth-child(1) img{
	width: 1.47rem;
	height: 1.47rem;
	margin: 0 auto;
	background: #E5E5E5;
	text-align: center;

}
.tousushangpin ul li:nth-child(2){

	width: 4rem;
	margin-left: 0.15rem;
	height: 1.6rem;

}
.tousushangpin ul li:nth-child(2) p:nth-child(1){
		width: 4rem;
		height: 0.4rem;
		font-size: 0.28rem;
		color: #333333;
		border: none;
		/*text-indent: 0.15rem;*/
		padding: 0;
	}
div.tousushangpin ul li:nth-child(2) p{
	color: #999999;
	border: 1px solid #E5E5E5;
	float: left;
	padding: 0 0.1rem;
	margin-top: 0.15rem;
	color: #999999;
	margin-left: 0.1rem;

}
.tousushangpin ul li:nth-child(3){
	width: 1.2rem;
	color: #999999;
	text-align: right;
	/*margin-top: 0.15rem;*/
	line-height: 0.35rem;
	margin-top: 0.4rem;

}
