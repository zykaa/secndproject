	<script>
		$(".qujiesuan").on("click", function() {

//			window.location.href = "../shangPinXiangQingLiJIGouMai/querendingdan.html"
			mui.openWindow({
				url:'../shangPinXiangQingLiJIGouMai/querendingdan.html',
				id:'../shangPinXiangQingLiJIGouMai/querendingdan.html',
				styles:{
				top:'0',
				bottom:'0px',
				width:'100%',
				height:'100%'
				},
				extras:{
				
				},
				show:{
				autoShow:true,
				aniShow:'slide-in-right',
				duration:'500'
				},
				waiting:{
				autoShow:true,
				title:'正在加载...',
					options:{
					width:'waiting-dialog-widht',
					height:'waiting-dialog-height'
					}
				}
			})
		})
		$(".tianjia").on("click", function() {
			$("header").hide();
			$(".gouwubottom").hide();
			$(".gouwuche_bianji").hide();
			$(".gouwuche_dianpu").hide();
			$(".i").show();
			$(".wanchengye").hide();
		})
		$(".i").on("click", function() {
			$("header").show();
			$(".gouwubottom").show();
			$(".gouwuche_bianji").show();
			$(".gouwuche_dianpu").show();
			$(".i").hide();
			//			$(".wanchengye")
		})

		$(".gerenzilaio ul li:nth-child(4)").on("click", function() {

			var zhuangtai = $(this).text()

			if(zhuangtai == "保存") {
				$(this).text("编辑")
				$('.gouwubottom').slideDown(500)
				$(".wanchengye").slideUp(500)
					//				
			} else if(zhuangtai == "编辑") {
				$(this).text("保存")

				$('.gouwubottom').slideUp(500)
				$(".wanchengye").slideDown(500)
			}

		})

		$(".shareNeiRong ul li").on("click", function() {

			$(".shareBg").fadeOut(500)
			$(".shareNeiRong").fadeOut(500)
		})

		$(".quxioa").on("click", function() {

			$(".shareBg").css("display", "none")
			$(".shareNeiRong").css("display", "none")
		})
		$(".wanchengye ul li:nth-child(2)").on("click", function() {

			$(".shareBg").css("display", "block")
			$(".shareNeiRong").css("display", "block")
		})

		$(".wanchengye li:nth-child(1) img").on("click", function() {
			$(this).css("display", "none").siblings("img").css("display", "block")
		})
		$(".wanchengye li:nth-child(1) img:nth-child(1)").on("click", function() {

			//		$(this).css("display","none").siblings("img").css("display","block")
			$(".GouWuChe_Shangpin li:nth-child(1) img:nth-child(2)").css("display", "block").siblings("img").css("display", "none")
			$(".gouwuche_dianpu li:nth-child(1) img:nth-child(2)").css("display", "block").siblings("img").css("display", "none")

		})
		$(".wanchengye li:nth-child(1) img:nth-child(2)").on("click", function() {

			//		$(this).css("display","none").siblings("img").css("display","block")
			$(".GouWuChe_Shangpin li:nth-child(1) img:nth-child(1)").css("display", "block").siblings("img").css("display", "none")
			$(".gouwuche_dianpu li:nth-child(1) img:nth-child(1)").css("display", "block").siblings("img").css("display", "none")

		})

		$(".wanchengye li:nth-child(4) ").on("click", function() {

			//		if(window.confirm("您确定删除该商品吗？")){
			////			$(".GouWuChe_Shangpin").remove()
			////			$()
			////$(".gouwuche_bianji ").remove()
			//			return true
			//		}else{
			//			return false
			//		}
			if($(".wanchengye li:nth-child(1) img:nth-child(2)").is(":visible") == true) {
				alert("该商品已删除")
				$(".gouwuche_bianji").remove()
					//   	$(".tianjin").parent().parent().remove()
					//   	$(".baozhu").parent().parent().remove()
			} else if($(".baozhu img:nth-child(2)").is(":visible") == true) {
				alert("该商品已删除")
				$(".baozhu").parent().parent().remove()
				$(".tianjin").parent().parent().css("marginTop", "0.88rem")
				return
			} else if($(".tianjin img:nth-child(2)").is(":visible") == true) {
				alert("该商品已删除")
				$(".tianjin").parent().parent().remove()
				return

			}

		})

		$(".baozhu img:nth-child(1)").on("click", function() {
			//		$(this).parent().parent().siblings(".GouWuChe_Shangpin").children("img:nth-child(2)").css("display","block").siblings("img").css("display","none")
			$(this).css("display", "none").siblings("img").css("display", "block")
			$(".baozhuli").children("img:nth-child(2)").css("display", "block").siblings("img").css("display", "none")
		})

