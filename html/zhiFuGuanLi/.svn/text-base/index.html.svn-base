    <script type="text/javascript">

   	$(function(){
   		 		//下拉菜单
	
		$(".liebiaozuanfu").on("click",function(e){
			$('.xialacaidan').toggle(300);
			e.stopPropagation();
		})
		
		$(document).click(function(){
	　　		$(".xialacaidan").hide(300);
		});
		$(document).scroll(function(){
			$(".xialacaidan").hide(300);
		})
		
		$(".xialacaidan").click(function(e){
	　　		e.stopPropagation();
		});
	
	//消息
		$('.xialacaidan li:nth-child(3)').on('click',function(){
//			window.location.href="../xiaoxiliebiao.html";
mui.openWindow({
				url:'../xiaoxiliebiao.html',
				id:'../xiaoxiliebiao.html',
				styles:{
				top:'0',
				bottom:'0px',
				width:'100%',
				height:'100%'
				},
				extras:{
				
				},
				show:{
				autoShow:true,
				aniShow:'slide-in-right',
				duration:'500'
				},
				waiting:{
				autoShow:true,
				title:'正在加载...',
					options:{
					width:'waiting-dialog-widht',
					height:'waiting-dialog-height'
					}
				}
			})
		})
		//首页
		$('.xialacaidan li:nth-child(2)').on('click',function(){
//			window.location.href ="../../index.html";
mui.openWindow({
				url:'../../index.html',
				id:'../../index.html',
				styles:{
				top:'0',
				bottom:'0px',
				width:'100%',
				height:'100%'
				},
				extras:{
				
				},
				show:{
				autoShow:true,
				aniShow:'slide-in-right',
				duration:'500'
				},
				waiting:{
				autoShow:true,
				title:'正在加载...',
					options:{
					width:'waiting-dialog-widht',
					height:'waiting-dialog-height'
					}
				}
			})
		})
   	})
  
	
    </script>
    <style type="text/css">
    	.thebigheader{
    		background: #f8f8f8;
    		width: 7.5rem;
        border-bottom: 0.8px solid #e5e5e5;
    	}
    	div.name .i1{
    		font-size: 0.24rem;
    	}
    </style>
</head>
<body style="display: block;">
	<!--01-->
