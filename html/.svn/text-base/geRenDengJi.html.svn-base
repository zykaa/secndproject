	<style>
		.theLogin p{
			width: 6.9rem;
			margin: 0 auto;
			height: 0.8rem;
			line-height: 0.8rem;
			font-size: 0.28rem;
			color: #757575;
		}
		.theLogin li{
			/*margin: 0;*/
			padding: 0;
		}
		.theLogin li span:nth-child(1){
			font-size: 0.24rem;
			text-indent: 0.3rem;
			display: block;
			width: 2rem;
			color: #757575;
			text-align: left;
			float: left;
			
		}
		.theLogin li input{
			width: 4.5rem;
			
		}
		
		

		.theLogin p{
			width: 6.9rem;
			margin: 0 auto;
			height: 0.8rem;
			line-height: 0.8rem;
			font-size: 0.28rem;
			color: #757575;
		}
		.theLogin li{
			/*margin: 0;*/
			padding: 0;
		}
		.theLogin li span:nth-child(1){
			font-size: 0.24rem;
			/*text-indent: 0.3rem;*/
			display: block;
			width: 2rem;
			color: #757575;
			text-align: left;
			float: left;
			
		}
		.theLogin li input{
			width: 4.5rem;
			border-radius: 0.03rem;
			text-align: left;
		}
		.theLogin ul li{
			background: none;border: none;
		}
		
		.theLogin ul li span{
			font-size: 0.25rem;
		}
     

	</style>
	<body>
		<header class="thebigheader"  style="position:fixed;z-index:1000;top:0;height: initial;" >
		<ul style=" width: 7.5rem;">
			<li style="margin-left: 0.3rem;">
				<i class="fa fa-angle-left mui-action-back " aria-hidden="true" style="font-size: 0.66rem;"></i>
			</li>
			
			<li style="text-align: center;width: 5.4rem; text-indent: 0 ;">
				个人用户登记
			</li>
		</ul>
		
		
	</header>
	
	
		<div class="theLogin" style="margin-top:0.88rem;">
		<p  style="color: #333;">
			请补全资料
		</p>
		<ul style="padding-bottom: 0.5rem;">
				<li style="margin: 0;">
					<!--<i class="fa fa-volume-control-phone" aria-hidden="true"></i>-->
					<span>真实姓名</span>
					<input type="text"  style="background: #F5F5F5;border: 0.03rem solid #E5E5E5;" placeholder="代用名"/>
					
				</li>
				<li style="margin-top: 0.2rem;">
					<!--<i class="fa fa-volume-control-phone" aria-hidden="true"></i>-->
					<span>身份证号</span>
					<input type="text"  style="background: #F5F5F5;border: 0.03rem solid #E5E5E5;" placeholder="430512198908131367" />
					
				</li>
				<li style="margin-top: 0.2rem; padding-bottom: 0.5rem; border-bottom: 1px solid #e5e5e5; ">
					<!--<i class="fa fa-volume-control-phone" aria-hidden="true"></i>-->
					<span style="text-align: left;">银行卡号</span>
					<input type="text"  style="background: #F5F5F5;border: 0.03rem solid #E5E5E5;margin-left: 0.2rem;" placeholder="612067 6987845634897"/>
					
				</li>
				
			
			
				
				</ul>
				<div class="shenfenzheng" style=" margin-bottom: 0;">
					<p style="width: 6.9rem;">身份证照片</p>
					<ul style="border-bottom: none;">
						<li style="background: #F5F5F5;border: 0.03rem solid #E5E5E5; height: 2.07rem;">
							<!--<p style="text-indent: 0.3rem;">正面</p>-->
					<img class="shengfenzhengzhengmian " src="../img/shenfenzhengmian.png"/>
						</li>
						<li style="margin-left: 0.2rem;background: #F5F5F5;border: 0.03rem solid #E5E5E5; height: 2.07rem;">
							<!--<p style="text-indent: 0.3rem;">反面</p>-->
							<img class="shenfenzhengfanmian " src="../img/shenfenfanmian.png"/>
						</li>
					</ul>
					
					
				</div>
						
			<div class="tijiaoshenhe" style="margin-top: 1rem;">
				提交审核
			</div>
			<p class="dengluzhong ziliaotijiaodengdai" style="float: left;">
				资料已提交，请等待...
			</p>
		</div>
		
	</body>
	<script type="text/javascript" src="../js/mui.js" ></script>
	<script type="text/javascript" src="../js/buju.js" ></script>
	<script type="text/javascript" src="../js/jquery-2.1.3.min.js" ></script>
	<script type="text/javascript" src="../js/tab.js" ></script>
	<script type="text/javascript" src="../js/clickmodeo.js" ></script>
	<!--<script type="text/javascript" src="../js/shangChuanXinXi.js" ></script>-->
	<script>
		$(".tijiaoshenhe")
		
	
	
		var wait=60;
			function time(o) {
			  if (wait == 0) {
			   o.removeAttribute("disabled");   
			   o.value="获取验证码";
			   wait = 60;
			  } else if(wait==50){
			  	
			  	$(".yanzhengmahuoqu").val(5845)
			  	
			  	o.value="获取验证码"
			  }
			  
			  else{
			
			   o.setAttribute("disabled", true);
			   o.value="重新发送(" + wait + ")";
			   wait--;
			   setTimeout(function() {
			    time(o)
			   },
			   1000)
			  }
			 }
//document.getElementById("btn").onclick=function(){time(this);}
	</script>
</html>
