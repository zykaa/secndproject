					$(".hhh").hide(500);
					$(".bottom").on("click",function(){
						setTimeout("window.location.href='../html/wodedingdan/wodedingdanDaiShouhuo.html'",0);
					})
				}
				$(this).children().filter(".box").attr("src","../img/money/okokok.png");
				$(this).siblings().children().filter(".box").attr("src","../img/money/nonono.png");		
			})
		})
	
