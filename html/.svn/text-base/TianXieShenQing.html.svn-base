		$(".tuikauntuihuo li img:nth-child(1)").on("click",function(){
			
			$(this).parent().siblings().children("img:nth-child(2)").css("display","none").siblings("img").css("display","block")
			
		})
		
		//下拉菜单
	
		$(".liebiaozuanfu").on("click",function(e){
			$('.xialacaidan').toggle(300);
			e.stopPropagation();
		})
		
		$(document).click(function(){
	　　		$(".xialacaidan").hide(300);
		});
		$(document).scroll(function(){
			$(".xialacaidan").hide(300);
		})
		
		$(".xialacaidan").click(function(e){
	　　		e.stopPropagation();
		});
	
