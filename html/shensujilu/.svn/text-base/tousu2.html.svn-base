	<script type="text/javascript">
		$(function(){
			$(".tuikauntuihuo .shang img").click(function(){
				if($(this).attr("src")=="../../img/shouhuodizhiyixuanze.png"){
					$(this).show();
					$(".tuikauntuihuo .wu img").attr("src","../../img/shouhoudizhixuanze.png");
				}else{
					$(this).show();
					$(this).attr("src","../../img/shouhuodizhiyixuanze.png");
					$(".tuikauntuihuo .wu img").attr("src","../../img/shouhoudizhixuanze.png")
				}
				
			})
			$(".tuikauntuihuo .wu img").click(function(){
				if($(this).attr("src")=="../../img/shouhuodizhiyixuanze.png"){
					$(this).show();
					$(".tuikauntuihuo .shang img").attr("src","../../img/shouhoudizhixuanze.png");
				}else{
					$(this).show();
					$(this).attr("src","../../img/shouhuodizhiyixuanze.png");
					$(".tuikauntuihuo .shang img").attr("src","../../img/shouhoudizhixuanze.png")
				}
				
			})
		})
			//下拉菜单
	
		$(".liebiaozuanfu").on("click",function(e){
			$('.xialacaidan').toggle(300);
			e.stopPropagation();
		})
		
		$(document).click(function(){
	　　		$(".xialacaidan").hide(300);
		});
		$(document).scroll(function(){
			$(".xialacaidan").hide(300);
		})
		
		$(".xialacaidan").click(function(e){
	　　		e.stopPropagation();
		});
	
